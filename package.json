{
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "lint:ci": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts --ignore-path .gitignore",
    "test": "ava",
    "coverage": "c8 --all --src=dist --skip-full ava"
  },
  "devDependencies": {
    "@ava/typescript": "~3.0.1",
    "@rushstack/eslint-patch": "~1.2.0",
    "@semantic-release/changelog": "~6.0.2",
    "@semantic-release/commit-analyzer": "~9.0.2",
    "@semantic-release/github": "~8.0.7",
    "@semantic-release/release-notes-generator": "~10.0.3",
    "@types/node": "~18.11.18",
    "@typescript-eslint/eslint-plugin": "~5.48.1",
    "@typescript-eslint/parser": "~5.48.1",
    "ava": "~5.1.1",
    "c8": "~7.12.0",
    "eslint": "~8.32.0",
    "eslint-config-prettier": "~8.6.0",
    "eslint-plugin-import": "~2.27.4",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-simple-import-sort": "~9.0.0",
    "eslint-plugin-unicorn": "~45.0.2",
    "prettier": "~2.8.3",
    "semantic-release": "~20.0.2",
    "tsx": "~3.12.2",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "axios": "1.2.3",
    "date-fns": "2.29.3",
    "discord.js": "14.7.1",
    "dotenv": "16.0.3",
    "fastest-levenshtein": "~1.0.16",
    "knex": "^2.4.1",
    "mysql2": "^3.0.1",
    "pbkdf2": "^3.1.2"
  },
  "ava": {
    "utilizeParallelBuilds": true,
    "typescript": {
      "compile": "tsc",
      "rewritePaths": {
        "src/": "dist/"
      }
    }
  },
  "renovate": {
    "extends": [
      "local>wopian/renovate-config"
    ]
  },
  "packageManager": "yarn@3.3.1"
}
