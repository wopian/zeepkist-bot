{
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "pnpm migrate && tsx watch src/index.ts",
    "start": "pnpm migrate && tsx src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "lint:ci": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts --ignore-path .gitignore",
    "test": "ava",
    "coverage": "c8 --all --src=dist --skip-full ava",
    "migrate": "knex migrate:latest",
    "make": "knex migrate:make"
  },
  "dependencies": {
    "@twurple/api": "6.2.1",
    "@twurple/auth": "6.2.1",
    "@zeepkist/gtr-api": "3.12.1",
    "date-fns": "2.30.0",
    "discord.js": "14.13.0",
    "dotenv": "16.3.1",
    "knex": "2.5.1",
    "ky": "1.0.1",
    "mysql2": "3.6.1",
    "winston": "3.10.0",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@ava/typescript": "~4.1.0",
    "@rushstack/eslint-patch": "~1.5.0",
    "@semantic-release/changelog": "~6.0.3",
    "@semantic-release/commit-analyzer": "~11.0.0",
    "@semantic-release/github": "~9.2.0",
    "@semantic-release/release-notes-generator": "~12.0.0",
    "@types/node": "~20.7.0",
    "@typescript-eslint/eslint-plugin": "~6.7.0",
    "@typescript-eslint/parser": "~6.7.0",
    "ava": "~5.3.1",
    "c8": "~8.0.0",
    "eslint": "~8.50.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-import": "~2.28.0",
    "eslint-plugin-prettier": "~5.0.0",
    "eslint-plugin-simple-import-sort": "~10.0.0",
    "eslint-plugin-unicorn": "~48.0.0",
    "prettier": "~3.0.0",
    "semantic-release": "~22.0.0",
    "tsx": "~3.12.7",
    "typescript": "~5.2.0"
  },
  "ava": {
    "utilizeParallelBuilds": true,
    "typescript": {
      "compile": "tsc",
      "rewritePaths": {
        "src/": "dist/"
      }
    }
  },
  "renovate": {
    "extends": [
      "local>wopian/renovate-config"
    ],
    "packageRules": [
      {
        "matchPackagePrefixes": [
          "@twurple"
        ],
        "groupName": "twurple"
      }
    ]
  },
  "packageManager": "pnpm@8.8.0"
}
